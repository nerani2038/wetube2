extends layouts/main.pug
include mixins/videoBlock.pug

block content  
    .search__header
        h3 Searching for #{searchingBy}
    .serch__videos
        if videos.length === 0
            h5 No Videos Found
        each item in videos         
            +videoBlock({
                title:item.title,
                views:item.views,
                fileUrl:item.fileUrl,
                id:item.id
            })